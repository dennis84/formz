{
    "name": "dennis84/formz",
    "description": "Formz is a simple form mapper for PHP. This library is based on the ScalaForms by Playframework.",
    "keywords": ["form"],
    "homepage": "http://github.com/dennis84/formz",
    "license": "MIT",
    "authors": [
        {
            "name": "Dennis Dietrich",
            "email": "d.dietrich84@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/event-dispatcher": "2.4.*"
    },
    "require-dev": {
        "symfony/http-foundation": "2.4.*",
        "symfony/validator": "2.4.*",
        "symfony/finder": "2.4.*",
        "doctrine/common": "2.3.*",
        "twig/twig": "1.*"
    },
    "autoload": {
        "psr-0": { "Formz": "src/" }
    },
    "minimum-stability": "dev"
}
